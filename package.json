{
  "name": "react-youtube",
  "version": "1.0.0",
  "description": "YouTube functionality with Netflix UI",
  "scripts": {
    "install:client": "cd client && npm install",
    "install:server": "cd server && npm install",
    "install:all": "npm run install:client && npm run install:server",
    "dev": "concurrently \"npm run dev --prefix client\" \"npm run dev --prefix server\"",
    "dev:client": "npm run dev --prefix client",
    "dev:server": "npm run dev --prefix server"
  },
  "keywords": [
    "react",
    "youtube",
    "netflix",
    "video"
  ],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "concurrently": "^8.2.2",
    "nodemon": "^3.1.10"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.922.0",
    "aws-sdk": "^2.1692.0",
    "bcrypt": "^6.0.0",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "express": "^5.1.0",
    "jsonwebtoken": "^9.0.2",
    "mongodb": "^6.20.0",
    "mongoose": "^8.19.3",
    "multer": "^2.0.2",
    "multer-s3": "^3.0.1",
    "swiper": "^12.0.3"
  }
}
